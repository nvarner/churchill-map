{
    "compilerOptions": {
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "h",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictPropertyInitialization": true,
        "alwaysStrict": true,
        "incremental": true,
        "importHelpers": true
    }
}